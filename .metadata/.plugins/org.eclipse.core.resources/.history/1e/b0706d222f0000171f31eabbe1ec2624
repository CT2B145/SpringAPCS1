public class SpreadsheetLocation implements Location
{
	private String name;
    @Override
    public int getRow()
    { // FIXME fix variables
    	int j;
    	if(name.charAt(0)>=0){
    		String i = name.toLowerCase();
    		char row = i.charAt(0);
    		j  = (char)row;  		
    	}
        return j;
    }

    @Override
    public int getCol()
    {
    	
        return 0;
    }
    
    public SpreadsheetLocation(String cellName)
    {	
     this.name = cellName;
//    	Location loc = new SpreadsheetLocation(cellName);
//    	loc.getCol();
//    	loc.getRow();
    }

}
